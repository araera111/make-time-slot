{
	"env": {
		"es2021": true,
		"node": true,
		"jest/globals": true
	},
	"extends": ["airbnb-base", "airbnb-typescript/base", "plugin:jest/recommended", "prettier"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"project": "./tsconfig.json"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"import/prefer-default-export": 0
	}
}
